<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\Blizzard_SharedXML\UI.xsd">
	<Frame name="EncounterWarningsViewTemplate" mixin="EncounterWarningsViewMixin" inherits="ResizeLayoutFrame" virtual="true" hidden="true">
		<Size x="1" y="1"/>
		<Layers>
			<Layer level="OVERLAY">
				<FontString parentKey="Text" inherits="EncounterWarningsTextElementTemplate">
					<KeyValues>
						<KeyValue key="minLineHeight" value="16" type="number"/>
					</KeyValues>
					<Anchors>
						<Anchor point="CENTER"/>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
		<Frames>
			<Frame parentKey="LeftIcon" inherits="EncounterWarningsIconElementTemplate">
				<Anchors>
					<Anchor point="RIGHT" relativeKey="$parent.Text" relativePoint="LEFT" x="-20"/>
				</Anchors>
			</Frame>
			<Frame parentKey="RightIcon" inherits="EncounterWarningsIconElementTemplate">
				<Anchors>
					<Anchor point="LEFT" relativeKey="$parent.Text" relativePoint="RIGHT" x="20"/>
				</Anchors>
			</Frame>
		</Frames>
		<Animations>
			<AnimationGroup parentKey="SwingAnimation" inherits="EncounterWarningsSwingAnimationGroupTemplate"/>
		</Animations>
		<Scripts>
			<OnLoad method="OnLoad"/>
			<OnShow method="OnShow"/>
			<OnHide method="OnHide"/>
		</Scripts>
	</Frame>
</Ui>
