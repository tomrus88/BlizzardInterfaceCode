<!-- Initiative Progress Bar Threshold Templates -->
<!-- Threshold Normal -->
<Frame name="ProgressThresholdTemplate" mixin="ProgressThresholdMixin" useParentLevel="true" virtual="true">
	<Size x="20" y="20"/>
	<Layers>
		<Layer level="ARTWORK" textureSubLevel="1">
			<Texture parentKey="LineComplete" atlas="housing-dashboard-initiatives-fillbar-tickbar-complete" useAtlasSize="true" hidden="true">
				<Anchors>
					<Anchor point="CENTER" x="-2" y="12"/>
				</Anchors>
			</Texture>
			<Texture parentKey="LineIncomplete" atlas="housing-dashboard-initiatives-fillbar-tickbar-incomplete" useAtlasSize="true">
				<Anchors>
					<Anchor point="CENTER" x="-2" y="12"/>
				</Anchors>
			</Texture>
		</Layer>
	</Layers>
	<Frames>
		<Frame parentKey="Reward" enableMouse="true" frameStrata="HIGH" setAllPoints="true">
			<Layers>
				<Layer level="ARTWORK">
					<Texture parentKey="Icon">
						<Size x="30" y="30"/>
						<Anchors>
							<Anchor point="CENTER" x="-1" y="0"/>
						</Anchors>
					</Texture>
					<MaskTexture parentKey="CircleMask" atlas="CircleMask" hWrapMode="CLAMPTOBLACKADDITIVE" vWrapMode="CLAMPTOBLACKADDITIVE">
						<Size x="20" y="20"/>
						<Anchors>
							<Anchor point="CENTER" x="0" y="2"/>
						</Anchors>
						<MaskedTextures>
							<MaskedTexture childKey="Icon"/>
						</MaskedTextures>
					</MaskTexture>
				</Layer>
				<Layer level="OVERLAY">
					<!-- TODO: atlas link broken <Texture parentKey="IconBorder" hidden="true" atlas="housing-dashboard-fillbar-pip-incomplete" useAtlasSize="true"> -->
					<Texture parentKey="IconBorder" hidden="true" useAtlasSize="true">
					<Anchors>
							<Anchor point="CENTER" relativeKey="$parent.Icon" relativePoint="CENTER"/>
						</Anchors>
					</Texture>
				</Layer>
				<Layer level="OVERLAY" textureSubLevel="1">
					<Texture parentKey="EarnedCheckmark" atlas="housing-dashboard-small-checkmark" useAtlasSize="true" hidden="true">
						<Anchors>
							<Anchor point="TOPLEFT" relativePoint="CENTER" x="0" y="2"/>
						</Anchors>
					</Texture>
				</Layer>
			</Layers>
		</Frame>
	</Frames>
</Frame>

