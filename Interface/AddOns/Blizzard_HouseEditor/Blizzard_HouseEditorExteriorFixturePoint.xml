<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\Blizzard_SharedXML\UI.xsd">
	<ScopedModifier forbidden="true">
		<Button name="HousingExteriorFixturePointTemplate" mixin="HousingExteriorFixturePointMixin" motionScriptsWhileDisabled="true" virtual="true">
			<Size x="50" y="50"/>
			<Layers>
				<Layer level="ARTWORK">
					<Texture parentKey="Icon" atlas="Customization_Fixture_Node_Default" texelSnappingBias="0.0" snapToPixelGrid="false" setAllPoints="true" alpha="1" />
					<Texture parentKey="HoveredIcon" atlas="Customization_Fixture_Node_Hover" texelSnappingBias="0.0" snapToPixelGrid="false" setAllPoints="true" hidden="true"/>
				</Layer>
			</Layers>
			<Frames>
				<Frame parentKey="Rays1">
					<Size x="128" y="128"/>
					<Anchors>
						<Anchor point="CENTER"/>
					</Anchors>
					<Layers>
						<Layer level="ARTWORK">
							<Texture parentKey="Rays1" alpha="0.8" alphaMode="ADD" atlas="housing-layout-room-orb-ring-rays1">
								<Size x="104" y="104"/>
								<Anchors>
									<Anchor point="CENTER"/>
								</Anchors>
							</Texture>
							<MaskTexture parentKey="RaysMask" atlas="housing-layout-room-orb-ring-rays-mask" vWrapMode="CLAMPTOBLACKADDITIVE" hWrapMode="CLAMPTOBLACKADDITIVE">
								<Size x="100" y="100"/>
								<Anchors>
									<Anchor point="CENTER"/>
								</Anchors>
								<MaskedTextures>
									<MaskedTexture childKey="Rays1"/>
								</MaskedTextures>
							</MaskTexture>
						</Layer>
					</Layers>
					<Animations>
						<AnimationGroup parentKey="Anim" looping="REPEAT" setToFinalAlpha="true">
							<Rotation childKey="Rays1" duration="16" order="1" degrees="-360">
								<Origin point="CENTER"/>
							</Rotation>
							<Rotation childKey="RaysMask" duration="16" order="1" degrees="360">
								<Origin point="CENTER"/>
							</Rotation>
						</AnimationGroup>
					</Animations>
				</Frame>
				<Frame parentKey="Spinner">
					<Size x="128" y="128"/>
					<Anchors>
						<Anchor point="CENTER"/>
					</Anchors>
					<Layers>
						<Layer level="ARTWORK">
							<Texture parentKey="Spinner" alphaMode="ADD" atlas="housing-layout-room-orb-ring-spinner">
								<Size x="42" y="42"/>
								<Anchors>
									<Anchor point="CENTER"/>
								</Anchors>
							</Texture>
						</Layer>
					</Layers>
					<Animations>
						<AnimationGroup parentKey="Anim" looping="REPEAT" setToFinalAlpha="true">
							<Rotation childKey="Spinner" duration="3" order="1" degrees="-360">
								<Origin point="CENTER"/>
							</Rotation>
						</AnimationGroup>
					</Animations>
				</Frame>
				<Frame parentKey="Glow">
					<Size x="128" y="128"/>
					<Anchors>
						<Anchor point="CENTER"/>
					</Anchors>
					<Layers>
						<Layer level="ARTWORK">
							<Texture parentKey="Glow" alphaMode="ADD" atlas="housing-layout-room-orb-ring-outerglow">
								<Size x="44" y="44"/>
								<Anchors>
									<Anchor point="CENTER"/>
								</Anchors>
							</Texture>
						</Layer>
					</Layers>
				</Frame>
				<Frame parentKey="Rays2">
					<Size x="128" y="128"/>
					<Anchors>
						<Anchor point="CENTER"/>
					</Anchors>
					<Layers>
						<Layer level="ARTWORK">
							<Texture parentKey="Rays2" alpha="1" alphaMode="BLEND" atlas="housing-layout-room-orb-ring-rays2">
								<Size x="80" y="80"/>
								<Anchors>
									<Anchor point="CENTER"/>
								</Anchors>
							</Texture>
						</Layer>
					</Layers>
					<Animations>
						<AnimationGroup parentKey="Anim" looping="REPEAT" setToFinalAlpha="true">
							<Rotation childKey="Rays2" duration="16" order="1" degrees="360">
								<Origin point="CENTER"/>
							</Rotation>
						</AnimationGroup>
					</Animations>
				</Frame>
			</Frames>
			<Scripts>
				<OnEnter method="OnEnter"/>
				<OnLeave method="OnLeave"/>
				<OnClick method="OnClick"/>
			</Scripts>
		</Button>
	</ScopedModifier>
</Ui>
