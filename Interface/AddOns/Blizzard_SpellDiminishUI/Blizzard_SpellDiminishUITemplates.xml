<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\..\..\..\WoW\Data\Interface\AddOns\Blizzard_SharedXML\UI.xsd">

	<Frame name="SpellDiminishStatusTrayItemTemplate" mixin="SpellDiminishStatusTrayItemMixin" virtual="true">
		<Size x="26" y="26"/>
		<Layers>
			<Layer level="OVERLAY">
				<Texture parentKey="Icon" file="Interface\ICONS\INV_Misc_QuestionMark.blp" setAllPoints="true"/>
			</Layer>			
		</Layers>
		<Frames>
			<Cooldown parentKey="Cooldown" reverse="true" setAllPoints="true">
				<SwipeTexture>
					<Color r="0" g="0" b="0" a="0.85"/>
				</SwipeTexture>
			</Cooldown>
			<Frame parentKey="ImmunityIndicator" hidden="true">
				<Size x="26" y="26"/>
				<Anchors>
					<Anchor point="CENTER" relativeKey="$parent.Icon" relativePoint="TOP"/>
				</Anchors>
				<Layers>
					<Layer level="ARTWORK">
						<Texture parentKey="Icon" atlas="GM-icon-role-tank" setAllPoints="true"/>
					</Layer>
				</Layers>
			</Frame>
		</Frames>
		<Scripts>
			<OnLoad method="OnLoad"/>
		</Scripts>
	</Frame>

	<Frame name="SpellDiminishStatusTrayTemplate" mixin="SpellDiminishStatusTrayMixin" inherits="ResizeLayoutFrame" virtual="true">
		<Size x="1" y="1"/>
		<KeyValues>
			<KeyValue key="minimumWidth" value="30" type="number"/>
			<KeyValue key="minimumHeight" value="30" type="number"/>
			<KeyValue key="widthPadding" value="2" type="number"/>
			<KeyValue key="heightPadding" value="2" type="number"/>
			<KeyValue key="spellDiminishRuleset" value="Enum.SpellDiminishRuleset.PvP" type="global"/>
		</KeyValues>
		<Scripts>
			<OnLoad method="OnLoad"/>
			<OnShow method="OnShow"/>
			<OnHide method="OnHide"/>
			<OnEvent method="OnEvent"/>
		</Scripts>
	</Frame>
</Ui>
