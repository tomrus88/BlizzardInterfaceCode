	<Frame name="MoveHouseConfirmationDialog" mixin="MoveHouseConfirmationDialogMixin" toplevel="true" frameStrata="DIALOG" hidden="true" parent="UIParent" inherits="TranslucentFrameTemplate">
		<Size x="400" y="350"/>
		<Anchors>
			<Anchor point="CENTER"/>
		</Anchors>
		<Scripts>
			<OnLoad method="OnLoad"/>
			<OnShow method="OnShow"/>
		</Scripts>
		<Layers>
			<Layer level="ARTWORK">
				<FontString parentKey="ConfirmationText" inherits="Game20Font" text="HOUSING_CORNERSTONE_MOVE_DIALOG">
					<Anchors>
						<Anchor point="TOPLEFT" x="15" y="-15"/>
						<Anchor point="BOTTOMRIGHT" relativePoint="RIGHT" x="-15" y="15"/>
					</Anchors>
				</FontString>
				<FontString parentKey="HouseToMoveLabel" inherits="Game15Font_Shadow" text="HOUSING_CORNERSTONE_HOUSE_TO_MOVE">
					<Anchors>
						<Anchor point="TOPLEFT" relativePoint="LEFT" x="25"/>
					</Anchors>
				</FontString>
				<FontString parentKey="HouseToMoveText" inherits="Game15Font_Shadow" text="house name here">
					<Anchors>
						<Anchor point="TOPLEFT" relativeKey="$parent.HouseToMoveLabel" relativePoint="BOTTOMLEFT" x="5"/>
					</Anchors>
				</FontString>
				<FontString parentKey="PriceLabel" inherits="Game15Font_Shadow" text="HOUSING_CORNERSTONE_PRICE">
					<Anchors>
						<Anchor point="TOPLEFT" relativeKey="$parent.HouseToMoveLabel" relativePoint="BOTTOMLEFT" y="-30"/>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
		<Frames>
			<Frame parentKey="PriceMoneyFrameOriginal" inherits="SmallMoneyFrameTemplate" frameLevel="0" hidden="false">
				<Anchors>
					<Anchor point="LEFT" relativeKey="$parent.PriceLabel" relativePoint="RIGHT" x="5"/>
				</Anchors>
			</Frame>
			<Frame parentKey="OriginalStrikethrough" frameLevel="2">
				<Anchors>
					<Anchor point="TOPLEFT" relativeKey="$parent.PriceMoneyFrameOriginal"/>
					<Anchor point="BOTTOMRIGHT" relativeKey="$parent.PriceMoneyFrameOriginal"/>
				</Anchors>
				<Layers>
					<Layer level="OVERLAY">
						<Texture parentKey="PriceStrikethrough" atlas="pricestrikethrough-gray" useAtlasSize="true"/>
					</Layer>
				</Layers>
			</Frame>
			<Frame parentKey="PriceMoneyFrameDiscount" inherits="SmallMoneyFrameTemplate" hidden="false">
				<Anchors>
					<Anchor point="LEFT" relativeKey="$parent.PriceMoneyFrameOriginal" relativePoint="RIGHT" x="5"/>
				</Anchors>
			</Frame>
			<Button parentKey="ConfirmButton" inherits="UIPanelButtonTemplate">
				<Size x="152" y="42"/>
				<Anchors>
					<Anchor point="BOTTOMRIGHT" relativePoint="BOTTOM" x="-5" y="20"/>
				</Anchors>
				<NormalFont style="GameFontNormalMed2"/>
				<HighlightFont style="GameFontNormalMed2"/>
				<DisabledFont style="GameFontDisableMed2"/>
				<HighlightTexture inherits="UIPanelButtonHighlightTexture"/>
			</Button>
			<Button parentKey="CancelButton" inherits="UIPanelButtonTemplate">
				<Size x="152" y="42"/>
				<Anchors>
					<Anchor point="BOTTOMLEFT" relativePoint="BOTTOM" x="5" y="20"/>
				</Anchors>
				<NormalFont style="GameFontNormalMed2"/>
				<HighlightFont style="GameFontNormalMed2"/>
				<DisabledFont style="GameFontDisableMed2"/>
				<HighlightTexture inherits="UIPanelButtonHighlightTexture"/>
			</Button>
		</Frames>
	</Frame>

